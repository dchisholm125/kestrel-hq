{
  "name": "@dchisholm125/kestrel-protocol",
  "version": "1.0.0",
  "description": "The Kestrel Protocol's flagship Guardian module for assessing orders and profitable transactions.",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:unit": "npm run build && mocha dist/test/unit/**/*.spec.js --exit",
    "test:integration": "npm run build && mocha dist/test/integration/**/*.spec.js --exit",
    "compile:contracts": "ts-node scripts/compile-contracts.ts",
    "lint": "eslint --ext .ts src",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "tsc -p tsconfig.json",
    "test:connector": "node dist/scripts/connector-test.js",
    "start:dev": "bash ./start-dev.sh",
    "start:broadcaster": "node src/scripts/local-ws-broadcaster.js",
    "dev:replay": "bash -c 'set -a; [ -f src/.env ] && source src/.env; set +a; exec ts-node src/scripts/replay.ts \"$@\"' --"
  },
  "dependencies": {
    "ethers": "^6.15.0",
    "express": "^5.1.0",
    "viem": "^2.37.2",
    "axios": "^1.7.7",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/express": "^5.0.3",
    "@types/mocha": "^10.0.1",
    "@types/sinon": "^10.0.20",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "chai": "^4.3.8",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "mocha": "^10.2.0",
    "prettier": "^3.6.2",
    "sinon": "^17.0.1",
    "solc": "^0.8.25",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.5",
    "typescript": "^5.5.0"
  }
}
