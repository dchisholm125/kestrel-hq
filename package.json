{
  "name": "kestrel-aerie",
  "version": "0.1.0",
  "description": "Kestrel Aerie SDK - OnChainScanner module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "jest --passWithNoTests",
    "test:unit": "jest --testPathPattern=__tests__/unit --runInBand",
    "test:integration": "RUN_INTEGRATION=true jest --testPathPattern=__tests__/integration --runInBand --detectOpenHandles"
  },
  "keywords": ["ethers", "ethereum", "scanner", "blocks", "pending-transactions"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ethers": "^6.13.1",
    "axios": "^1.6.8",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "ts-jest": "^29.2.5",
    "typescript": "^5.4.0",
    "jest": "^29.7.0",
    "nock": "^13.5.4"
  }
}
