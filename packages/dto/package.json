{
  "name": "@kestrel-hq/dto",
  "version": "0.2.0",
  "private": false,
  "description": "Data Transfer Objects, enums, and shared types for Kestrel SDKs and services.",
  "type": "commonjs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": ["dist", "README.md", "LICENSE"],
  "scripts": {
    "build": "tsc -b",
    "postbuild": "node ../../tools/build/dualize.js",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run build && pnpm test --if-present"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://<your-private-registry>"
  },
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "typescript": "^5.2.2"
  }
}
