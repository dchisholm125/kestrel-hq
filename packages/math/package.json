{
  "name": "@kestrel-hq/math",
  "version": "0.2.0",
  "private": false,
  "description": "Pure math helpers for simulation, scoring, and fee accounting (no side-effects)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": ["dist", "README.md", "LICENSE"],
  "scripts": {
    "build": "tsc -b",
    "postbuild": "node ../../tools/build/dualize.js",
    "test": "jest --passWithNoTests",
    "prepublishOnly": "pnpm run build && pnpm test --if-present"
  },
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "typescript": "^5.5.0",
    "ts-jest": "^29.2.5",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12"
  }
}
